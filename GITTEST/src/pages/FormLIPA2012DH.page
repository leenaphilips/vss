<apex:page id="LIPA2012DH" showHeader="false" standardStyleSheets="false" 
	action="{!initializeSession}" controller="FormLIPA2012DH" cache="false" expires="0">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
		<meta http-equiv="pragma" content="no-cache" />
		<meta http-equiv="cache-control" content="no-cache" />
		<meta http-equiv="expires" content="0" />
		<title>LIPA 2012 Dehumidifier Rebate Form</title>
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.min.js" type="text/javascript" language="javascript"></script>
		<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.16/jquery-ui.js" type="text/javascript" language="javascript"></script>
		<script src="{!URLFOR($Resource.LIPA2012, 'js/jquery.tooltip.js')}" type="text/javascript" language="javascript"></script>
		<link href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8/themes/base/jquery-ui.css" rel="stylesheet" type="text/css"/>
		<style type="text/css">
			body { font-family:Arial, Helvetica, sans-serif;  color:#333; background-color:#fff; margin:0px; }
			div {  }
			h1 { font-size:16px; font-weight:bold; }
			p { margin:0px; }
			#container { width:600px; margin:0px auto; padding:20px 40px; background-color:#1783c5;  }
			.header { background-color:#fff; height:100px; padding:10px; -moz-border-radius: 5px 5px 5px 5px; -webkit-border-radius: 5px 5px 5px 5px; 
				border-radius: 5px 5px 5px 5px; }
			#main { }
			#content-top { color:#fff; font-size:11px;   }
			#content-top .highlight { background-color:#fae62e; padding:5px; color:#333; margin:5px 0px; }
			#content-top #mailto { font-size:13px; }
			#content-main  { margin-top:20px; background-color:#fff; padding:10px; -moz-border-radius: 5px 5px 5px 5px; -webkit-border-radius: 5px 5px 5px 5px;
				border-radius: 5px 5px 5px 5px; }
			#content-main div { margin:10px 0px; }
			#content-main h2 { font-size:16px; margin:0px 0px 0px 0px; }
			/* form elements */
			form { font-size:.875em; }
			fieldset { border:none; padding:15px 0px 15px 0px; border-top:0px #2fb457 solid; margin:0px; }
			label { display:inline-block; width:125px; vertical-align:inherit; text-align:right; padding-right:10px; }
			label.autowidth { width:auto; }
			form span { display:inline-block; border-bottom:1px #333 solid; margin-right:30px; }
			form span.noborder, form span .noborder, form .noborder span, form .noborder { border:none; }
			input[type="text"] {  border:none; border-bottom:0px #333 solid; font-size:1em; width:auto;  }
			input:focus { background-color:#ccc; }
			.longtext { width:28em; }
			.mediumtext { width:18em; }
			.mediumshorttext { width:9em; }
			.shorttext { width:3em; }
			.button  { -moz-border-radius: 3px 3px 3px 3px; background-color: #E8E8E9; border-color: #B5B5B5 #B5B5B5 #7F7F7F; border-style: solid; border-width: 1px; color: #333333;
				font-size: 0.9em; font-weight: bold; margin: 1px; padding: 2px 3px; text-decoration: none; }
			#tooltip { position: absolute; 	z-index: 3000; border: 1px solid #111; background-color: #EEEEEB; padding: 5px; opacity: 0.85; font-size:11px;  }
			#tooltip h3 { font-size:11px; margin:0px; }
			.error-icon { visibility:visible; position:absolute; margin-left:-20px; }
			.question-icon { visibility:visible; position:absolute; cursor:pointer; margin-left:0px; }
			#footer { width:680px; background-color:#fff; margin:0px auto; padding:20px 0px; text-align:right;  }		
			.alwaysblack {color: black; }
			#need-help { float:right; padding:5px; margin-top:-5px; background-color:#2fb457; font-weight:bold; color:#fff; font-style:italic; border:none; cursor:pointer; }
			.lipa-dialog { display:none; }
			.ui-dialog-titlebar { background-color:#1783c5; background-image:none; color:#fff; border-color:#1783c5; }
			.ui-dialog-content { padding:1em; }
			.printonly { display:none; }
			.ui-widget-overlay { opacity:.8; filter:Alpha(Opacity=80); }
             #content-main .submitbutton { 
				background: #296737; /* for non-css3 browsers  */
				filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#8DC63F', endColorstr='#527424'); /* for IE  */
				background: -webkit-gradient(linear, left top, left bottom, from(#8DC63F), to(#527424)); /* for webkit browsers  */
				background: -moz-linear-gradient(top,  #8DC63F,  #527424); /* for firefox 3.6+   */
				width:300px; text-align:center; padding:7px; margin:20px 0px;
				color:#fff; text-decoration:none; font-size:18px; font-weight:normal; z-index:3; display:block;
				border-radius:10px; -webkit-border-radius:10px; -moz-border-radius:10px;
				border: 1px solid #527424;
				cursor:pointer;
				-moz-box-shadow: 0 1px 1px rgba(0,0,0,.1) inset;
				-webkit-box-shadow: 0 1px 1px rgba(0,0,0,.1) inset;
				box-shadow: 0 1px 1px rgba(0,0,0,.1) inset;
				position:relative;
				text-shadow: 0px -1px 1px #000;
			}
			#content-main .submitbutton::after {
				background: none;
				border: 3px solid #E2E6CF;
				-moz-border-radius: 15px;
				-webkit-border-radius: 15px;
				border-radius: 15px;
				content: "";
				position: absolute;
				top: -4px;
				right: -4px;
				bottom: -4px;
				left: -4px;
			}
			#content-main .submitbutton:hover { background:#527424; }
			#content-main .submitbutton.disabled, #content-main .submitbutton.disabled:hover { background:#999; cursor:text; border-color:#999; text-shadow:none; }
		</style>
		<style type="text/css" media="print">
			.printonly { display:block !important; }
			.pagebreak { page-break-after:always; }
			#checklist { width:600px; margin:2em auto; }
			#checklist p { margin-bottom: 10px; }
			#checklist li { list-style:none; margin-bottom:4px; }
			.error-icon, .question-icon, .button { display:none; }
			#need-help { display:none; }
		</style>
		<script>
			$(function() {
				$( "#purchase_date" ).datepicker();
			});
			$(document).ready(function(){ 
				$('.error-icon').tooltip({
					track: true,
					delay: 0,
					showURL: false,
					showBody: " - ",
					fade: 250
				});
			});
		</script>
		<script>
        	var documentCount = 0;
			function checkMe() {
				// be careful in defining local tests - FDC runs rampant on form as written
				// so DOM ids and structure may be different from expected
				// transfer purchase date to FDC field
				document.getElementById("LIPA2012DH:rebateForm:frmPurchDate").value = document.getElementById("purchase_date").value
                submitButton();
				return true;
			}
			function runOnLoad() {
				// if 'Other' status, redirect
				var sobj = document.getElementById("LIPA2012DH:rebateForm:frmStatus");
				if (sobj.value == 'Other') {
					var nurl = window.location.href;
					window.location.href = nurl.replace("?Rebate","_Status?Rebate")+'&FormCode=LIDH2012';
				}
				// determine if there is any message to display (e.g. AJAX result)
				var obj = document.getElementById("LIPA2012DH:rebateForm:frmLoadMessage");
				if (obj.value.length > 0) alert(obj.value);
				// determine if there is an account number error/warning, show dialog
				var eobj = document.getElementById("LIPA2012DH:rebateForm:frmShowInvoice");
				if (eobj.value == 'Yes') {
					$('#frmAcctNumExampleDialog').dialog( { modal:true, width:750, });
				}
           		doSubmitVia(document.getElementById("LIPA2012DH:rebateForm:submitVia").value);
				if (document.getElementById("LIPA2012DH:rebateForm:formOpen").value == 'false') {
					// reservations are not supported, so same actions always
            		document.getElementById("headertext").style.display = "none";
            		document.getElementById("subviarow").style.display = "none";
            		document.getElementById("emailreqrow").style.display = "none";
            		document.getElementById("subclosedrow").style.display = "block";
            		document.getElementById("custinfo").style.display = "none";
            		document.getElementById("mailinfo").style.display = "none";
            		document.getElementById("emailinfo").style.display = "none";
            		document.getElementById("surveyinfo").style.display = "none";
            		document.getElementById("modelinfo").style.display = "none";
            		document.getElementById("storeinfo").style.display = "none";
            		document.getElementById("purchinfo").style.display = "none";
            		document.getElementById("suptdocrow").style.display = "none";
            		document.getElementById("submitbutton").style.display = "none";
				}
				// replace visible date from value exchanged w/server
				document.getElementById("purchase_date").value = document.getElementById("LIPA2012DH:rebateForm:frmPurchDate").value;
                mailCheck(document.getElementById("LIPA2012DH:rebateForm:mailSame").checked); // set visible, invisible fields
                modelCheck(document.getElementById("LIPA2012DH:rebateForm:manualModelDH").checked); // set visible, invisible fields
                storeCheck(document.getElementById("LIPA2012DH:rebateForm:manualStore").checked); // set visible, invisible fields
				var rn = document.getElementById("LIPA2012DH:rebateForm:frmRebateNum").innerHTML;
				if (rn.length < 2) {
					document.getElementById("refnumdiv").style.display = "none";
				} else {
            		document.getElementById("headertext").style.display = "none";
					document.getElementById("refnumdiv").style.display = "block";
            		document.getElementById("subviarow").style.display = "none";
            		document.getElementById("emailreqrow").style.display = "none";
            		document.getElementById("subclosedrow").style.display = "none";
            		document.getElementById("suptdocrow").style.display = "none";
				}
				if (document.getElementById("LIPA2012DH:rebateForm:frmReadOnly").value == "yes") {
					document.getElementById("purchase_date").disabled = true;
                   jQuery('.submitbutton').addClass('disabled');
				}
			}
			function modelCheck(ischecked) {
				// alert(ischecked);
				if (ischecked) {
					document.getElementById("selmandiv").style.display = "none";
					document.getElementById("selmoddiv").style.display = "none";
					document.getElementById("manmandiv").style.display = "block";
					document.getElementById("manmoddiv").style.display = "block";
				} else {
					document.getElementById("selmandiv").style.display = "block";
					document.getElementById("selmoddiv").style.display = "block";
					document.getElementById("manmandiv").style.display = "none";
					document.getElementById("manmoddiv").style.display = "none";
				}
			}
			function storeCheck(ischecked) {
				// alert(ischecked);
				if (ischecked) {
					document.getElementById("selstorediv").style.display = "none";
					document.getElementById("manstorediv").style.display = "block";
				} else {
					document.getElementById("selstorediv").style.display = "block";
					document.getElementById("manstorediv").style.display = "none";
				}
			}
			function mailCheck(ischecked) {
				// alert(ischecked);
				if (ischecked) {
					document.getElementById("manmaildiv").style.display = "none";
				} else {
					document.getElementById("manmaildiv").style.display = "block";
				}
			}
            function doSubmitVia(opt) {
            	// alert(opt);
            	if (opt == 'Electronic') {
            		document.getElementById("via_mail").checked = false;
            		document.getElementById("via_elect").checked = true;
                    document.getElementById("LIPA2012DH:rebateForm:emailRequest").checked = false; // initialize
                    if (documentCount == 0) {
                        document.getElementById("emailreqrow").style.display = "block";
                    }
            		document.getElementById("suptdocrow").style.display = "block";
            	} else {
            		document.getElementById("via_mail").checked = true;
            		document.getElementById("via_elect").checked = false;
            		document.getElementById("emailreqrow").style.display = "none";
            		document.getElementById("suptdocrow").style.display = "none";
            	}
           		document.getElementById("LIPA2012DH:rebateForm:submitVia").value = opt;
            }
            function updateDocumentCount(n) {
                // the upload area is visible if this was called
                documentCount = n; // if needed later
                if (n > 0) {
                    document.getElementById("emailreqrow").style.display = "none";
                    document.getElementById("LIPA2012DH:rebateForm:emailRequest").checked = false; // just in case
                } else {
                    document.getElementById("emailreqrow").style.display = "table-row";
                }
            }
            function requestEmailCheck(ischecked) {
                if (ischecked) {
                    document.getElementById("suptdocrow").style.display = "none";
                } else {
                    document.getElementById("suptdocrow").style.display = "block";
                }
            }
		</script>
		<script type="text/javascript">
			jQuery(document).ready(function($) {
				jQuery('#need-help').click(function() {
					$('#help-dialog').dialog( { modal:true, width:500, });
				});
				jQuery('.question-icon').click(function() {
					$('#frmAcctNumExampleDialog').dialog( { modal:true, width:750, });
				});
			});
		</script>
	</head>
	<body onload="runOnLoad();">
		<div class="printonly pagebreak" id="checklist">
			<div class="header">
		    	<img src="{!URLFOR($Resource.LIPA2012, 'images/lipa.gif')}" alt="LIPA" style="float:left;" />
		    </div>
		    <div class="text" style="clear:both">
            	<apex:outputText value="{!checklistContent}" escape="false" />
