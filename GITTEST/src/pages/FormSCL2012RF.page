        <script>
            $(function() {
                $( ".datefield" ).datepicker();
            });
            $(document).ready(function(){ 
                $('.error-icon').tooltip({
                    track: true,
                    delay: 0,
                    showURL: false,
                    showBody: " - ",
                    fade: 250
                });
            });
        </script>
        <script>
            function checkMe() {
                // be careful in defining local tests - FDC runs rampant on form as written
                // so DOM ids and structure may be different from expected
                // transfer purchase date to FDC field
                if (document.getElementById("SCL2012RF:rebateForm:frmReadOnly").value == 'yes') return false;
                document.getElementById("SCL2012RF:rebateForm:frmPurchDate").value = document.getElementById("purchase_date").value
                document.getElementById("SCL2012RF:rebateForm:frmHaulawayDate").value = document.getElementById("haulaway_date").value
                submitButton();
                return true;
            }
            function runOnLoad() {
                // if 'Other' status, redirect
                var sobj = document.getElementById("SCL2012RF:rebateForm:frmStatus");
                if (sobj.value == 'Other') {
                    var nurl = window.location.href;
                    window.location.href = nurl.replace("?Rebate","_Status?Rebate")+'&FormCode=SCLRF2012';
                    return; 
                }
                // determine if there is any message to display (e.g. AJAX result)
                var obj = document.getElementById("SCL2012RF:rebateForm:frmLoadMessage");
                if (obj.value.length > 0) alert(obj.value);
				// determine if there is an account number error/warning, show dialog
				var eobj = document.getElementById("SCL2012RF:rebateForm:frmShowInvoice");
				if (eobj.value == 'Yes') {
					$('#frmAcctNumExampleDialog').dialog( { modal:true, width:750, });
				}
                // replace visible date from value exchanged w/server
                document.getElementById("purchase_date").value = document.getElementById("SCL2012RF:rebateForm:frmPurchDate").value;
                document.getElementById("haulaway_date").value = document.getElementById("SCL2012RF:rebateForm:frmHaulawayDate").value;

                if (document.getElementById("SCL2012RF:rebateForm:manualStore").value == "Y") {
                    storeCheck(true);
                    document.getElementById("manualstorecb").checked = true;
                    storeChanged(document.getElementById("SCL2012RF:rebateForm:frmStoreName").value);
                } else {
                    storeCheck(false);
                    document.getElementById("manualstorecb").checked = false;
                    var stobj = document.getElementById("SCL2012RF:rebateForm:selStoreId");
                    if (stobj.selectedIndex == -1) {
	                    storeChanged('');
                    } else {
	                    storeChanged(stobj.options[stobj.selectedIndex].text);
                    }
                }
              
                if (document.getElementById("SCL2012RF:rebateForm:manualModel").value == "Y") {
                    modelCheck(true);
                    document.getElementById("manualmodelcb").checked = true;
                } else {
                    modelCheck(false);
                    document.getElementById("manualmodelcb").checked = false;
                }

                if (document.getElementById("SCL2012RF:rebateForm:manualHC").value == "Y") {
                    hcCheck(true);
                    document.getElementById("manualHCcb").checked = true;
                } else {
                    hcCheck(false);
                    document.getElementById("manualHCcb").checked = false;
                } 
                      
                var rn = document.getElementById("SCL2012RF:rebateForm:frmRebateNum").innerHTML;
                if (rn.length < 2) {
                    document.getElementById("refnumdiv").style.display = "none";
                } else {
                    document.getElementById("refnumdiv").style.display = "block";
                }
                
                if (document.getElementById("SCL2012RF:rebateForm:frmReadOnly").value == "yes") {
                    document.getElementById("purchase_date").disabled = true;
                    document.getElementById("haulaway_date").disabled = true;
                    document.getElementById("manualstorecb").disabled = true;
                    document.getElementById("manualmodelcb").disabled = true;
                    document.getElementById("manualHCcb").disabled = true;
                    // document.getElementById("mailsamecb").disabled = true;
                    jQuery('.submitbutton').addClass('disabled');
                    
                }
                 
                if (document.getElementById('SCL2012RF:rebateForm:frmAcctAddress1').value == '') { document.getElementById('SCL2012RF:rebateForm:frmAcctAddress1').value = 'Line 1'; } else if (document.getElementById('SCL2012RF:rebateForm:frmAcctAddress1').value != 'Line 1') { $('.address-field.frmAcctAddress1').addClass('entered'); }
                if (document.getElementById('SCL2012RF:rebateForm:frmAcctAddress2').value == '') { document.getElementById('SCL2012RF:rebateForm:frmAcctAddress2').value = 'Line 2'; } else if (document.getElementById('SCL2012RF:rebateForm:frmAcctAddress2').value != 'Line 2') { $('.address-field.frmAcctAddress2').addClass('entered'); }
                if (document.getElementById('SCL2012RF:rebateForm:frmMailAddress1').value == '') { document.getElementById('SCL2012RF:rebateForm:frmMailAddress1').value = 'Line 1'; } else if (document.getElementById('SCL2012RF:rebateForm:frmMailAddress1').value != 'Line 1') { $('.address-field.frmMailAddress1').addClass('entered'); }
                if (document.getElementById('SCL2012RF:rebateForm:frmMailAddress2').value == '') { document.getElementById('SCL2012RF:rebateForm:frmMailAddress2').value = 'Line 2'; } else if (document.getElementById('SCL2012RF:rebateForm:frmMailAddress2').value != 'Line 2') { $('.address-field.frmMailAddress2').addClass('entered'); }
                if (document.getElementById('SCL2012RF:rebateForm:frmStoreAddress1').value == '') { document.getElementById('SCL2012RF:rebateForm:frmStoreAddress1').value = 'Line 1'; } else if (document.getElementById('SCL2012RF:rebateForm:frmStoreAddress1').value != 'Line 1') { $('.address-field.frmStoreAddress1').addClass('entered'); }
                if (document.getElementById('SCL2012RF:rebateForm:frmStoreAddress2').value == '') { document.getElementById('SCL2012RF:rebateForm:frmStoreAddress2').value = 'Line 2'; } else if (document.getElementById('SCL2012RF:rebateForm:frmStoreAddress2').value != 'Line 2') { $('.address-field.frmStoreAddress2').addClass('entered'); }
               
            }
            function modelCheck(ischecked) {
                // alert(ischecked);
                var obj = document.getElementById("SCL2012RF:rebateForm:manualModel");
                if (ischecked) {
                    document.getElementById("selmandiv").style.display = "none";
                    document.getElementById("selmoddiv").style.display = "none";
                    document.getElementById("manmandiv").style.display = "block";
                    document.getElementById("manmoddiv").style.display = "block";
                    obj.value = "Y";
                } else {
                    document.getElementById("selmandiv").style.display = "block";
                    document.getElementById("selmoddiv").style.display = "block";
                    document.getElementById("manmandiv").style.display = "none";
                    document.getElementById("manmoddiv").style.display = "none";
                    obj.value = "N";
                }
            }
            function storeCheck(ischecked) {
                // alert(ischecked);
                var obj = document.getElementById("SCL2012RF:rebateForm:manualStore");
                if (ischecked) {
                    jQuery('#mandp').append( $('#date-purchased') );
                    document.getElementById("selstorediv").style.display = "none";
                    document.getElementById("manstorediv").style.display = "block";
                    obj.value = "Y";
                } else {
                    jQuery('#seldp').append( $('#date-purchased') );
                    document.getElementById("selstorediv").style.display = "block";
                    document.getElementById("manstorediv").style.display = "none";
                    obj.value = "N";
                }
            }
            function hcCheck(ischecked) {
                var obj = document.getElementById("SCL2012RF:rebateForm:manualHC");
                // alert(ischecked);
                if (ischecked) {
                    document.getElementById("selhcdiv").style.display = "none";
                    document.getElementById("manhcdiv").style.display = "block";
                    obj.value = "Y";
                }
                else {
                    document.getElementById("selhcdiv").style.display = "block";
                    document.getElementById("manhcdiv").style.display = "none";
                    obj.value = "N";
                }
            }
            
            function hcSigCheck(box, ischecked) {
            	if (box == 'A') {	
            		if (ischecked) {
            		//	document.getElementById("SCL2012RF:rebateForm:hcSigCheckA").value = 'Y';
            			document.getElementById("SCL2012RF:rebateForm:hcSigCheckB").value = 'N'; // uncheck the other
            			document.getElementById("hcSigCheckBcb").checked = false;
            		} else {
            		//	document.getElementById("SCL2012RF:rebateForm:hcSigCheckA").value = 'N';
            			// no change to B if A is unchecked
            		}
            	} else {
            		if (ischecked) {
            			document.getElementById("SCL2012RF:rebateForm:hcSigCheckB").value = 'Y';
            		//	document.getElementById("SCL2012RF:rebateForm:hcSigCheckA").value = 'N'; // uncheck the other
            			document.getElementById("hcSigCheckAcb").checked = false;
            		} else {
            			document.getElementById("SCL2012RF:rebateForm:hcSigCheckB").value = 'N';
            			// no change to A if B is unchecked
            		}
            	}
            }
                        
            function storeChanged(name) {
            	if ((name == '') || (name.toLowerCase().indexOf('sears') == -1)) {
	            	// alert('Process: not Sears');
                    document.getElementById("hcarea1").style.display = "table-row";
                    document.getElementById("hcarea2").style.display = "table-row";
                    document.getElementById("hcarea3").style.display = "table-row";
                    document.getElementById("hcarea4").style.display = "table-row";
            	} else {
            		// alert('Process: Sears');
                    document.getElementById("hcarea1").style.display = "none";
                    document.getElementById("hcarea2").style.display = "none";
                    document.getElementById("hcarea3").style.display = "none";
                    document.getElementById("hcarea4").style.display = "none";
            	}
            }
                        
            function focusValue(id,val) {
                if (document.getElementById("SCL2012RF:rebateForm:"+id).value == val) { 
                    document.getElementById("SCL2012RF:rebateForm:"+id).value = ''; 
                    $('.'+id).addClass('entered'); 
                }
                
            }
            
            function blurValue(id,val) {
                if (document.getElementById("SCL2012RF:rebateForm:"+id).value == '') { 
                    document.getElementById("SCL2012RF:rebateForm:"+id).value = val; 
                    $('.'+id).removeClass('entered'); 
                }
                
            }
            
            function fieldEntered(id,val) {
                if (document.getElementById("SCL2012RF:rebateForm:"+id).value != '' && document.getElementById("SCL2012RF:rebateForm:"+id).value != val) {
                    $('.'+id).addClass('entered');
                }
                else { 
                    $('.'+id).removeClass('entered');
                }
            }
            
            jQuery(document).ready(function($) {
                jQuery('#need-help').click(function() {
                    $('#help-dialog').dialog( { modal:true, width:500, });
                });
                jQuery('#seldp').append( $('#date-purchased') );
                jQuery('.question-icon').click(function() {
					$('#frmAcctNumExampleDialog').dialog( { modal:true, width:750, });
				});
            });
            

            
        </script>
    </head>
    <body onload="runOnLoad();">
        <div class="printonly pagebreak-after" id="checklist">
            <div class="header">
                <div class="scl-header">
                    <div class="logo"><img src="{!URLFOR($Resource.SCL2012, 'images/scl-logo.png')}" alt="Seattle City Light" style="float:left;" /></div>
                </div>
                <div class="efi-logo"><img src="{!URLFOR($Resource.SCL2012, 'images/efi-logo.gif')}" alt="EFI"  /></div>
            </div>
            <div class="text">
                <p>Thank you for submitting your Seattle City Light refrigerator rebate online.</p>
                <p>In order to finalize your rebate application, please do the following:</p>
                <ul>
                    <li><input type="checkbox" readonly="readonly" /> Print and sign the application form.</li>
                    <li><input type="checkbox" readonly="readonly" /> Attach a copy of your itemized receipt showing the rebated item. </li>
                    <li><input type="checkbox" readonly="readonly" /> Attach a copy of your receipt showing proof of recycling, OR include a signature from the haul-away company. </li>
                    <li><input type="checkbox" readonly="readonly" /> Provide the required serial number for your new appliance.</li>
                    <li><input type="checkbox" readonly="readonly" /> Submit your application no later than 90 days after the purchase of your appliance.</li>
                </ul>
                <table>
                	<tr>
	                	<td style="padding-right:40px;">
			                <p>Mail to:</p>
			                <p>EFI &mdash; Seattle City Light RF Rebate<br />
			                40 Washington St., Suite 2000<br />
			                Westborough, MA 01581</p>   
	                	</td>
	                	<td style="vertical-align:top;">
	                		<p>Questions?</p>
	                		<p>Contact us at 1-888-562-0120</p>
	                	</td>
                	</tr>
                </table>
            </div>
        </div>
        <div id="container">
            <div class="header">
                    <div class="logo"><img src="{!URLFOR($Resource.SCL2012, 'images/scl_fr_header.png')}" alt="Refrigerator Rebate" style="float:left;" /></div>
                    <div class="logo efi"><img src="{!URLFOR($Resource.SCL2012, 'images/scl_logo.png')}" alt="Seattle City Light" style="float:right;" /></div>
            </div>
            <div id="main">
                <div id="content-top">
                    <div id="need-help" class="screenonly">Need help?</div>
                    <h1>
                        <span>Get a $50 rebate on a qualified model when you recycle your old unit.</span>
                        <img src="{!URLFOR($Resource.SCL2012, 'images/scl_header.png')}" alt="Get a $50 rebate on a qualified model when you recycle your old unit." />
                    </h1>
                    <div class="text">
                        <h2 class="orange">To apply, follow these steps:</h2>
                        <ol class="orange">
                            <li><span class="normal"><strong>Your new refrigerator must be purchased between January 1, 2012 and December 31, 2012.</strong> Read the Terms and Conditions below 
                            to ensure you qualify.</span></li>
                            <li><span class="normal">Complete the rebate request application and attach a <strong>copy</strong> of itemized receipt. Receipt must include name of retailer, retailer 
                            address, phone number, purchase date, price, product manufacturer and model number.</span></li>
                            <li><span class="normal">You must provide documentation that the old unit has been recycled. Ask the haul-away company or drop-off site for a receipt and
								attach it to this form. In the case where a receipt is not available, a signature from a representative of the haul-away company or drop-
								off site is required. See signature field in form below. For more information about arranging recycling haul-away, please see Terms and
								Conditions on back of this form.</span></li>
                            <li><span class="normal">Sign and date the rebate form and send the completed form with itemized receipts postmarked <strong>within 90 days of purchase</strong> date to:
                            </span></li>
                            <div class = "normal">
                                <div class="mailto left">
                                    <p><span class="orange">Mail</span><br />
                                    EFI &mdash; Seattle City Light RF Rebate<br />
                                    40 Washington St., Suite 2000<br />
                                    Westborough, MA 01581</p>
                                </div>
                                <div class="mailto right">
                                    <p><span class="orange">For more information:</span><br />
                                    Email: SeattleAppliances@efi.org<br />
                                    Website: seattle.gov/appliances<br />
                                    Questions? Call 1-888-562-0120</p>
                                </div>
                            </div>
