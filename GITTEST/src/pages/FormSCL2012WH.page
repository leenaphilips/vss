            input.address-field.entered { visibility:visible; }
            #footer { display:none; }
            #container { width: 511pt; }
            form, .formtable, input[type="text"], select, h2 { font-size:8pt !important; }
            #content-top, .small { font-size:6pt !important}
            .logo img { max-width:250pt; }
            img { max-width:480px; }
