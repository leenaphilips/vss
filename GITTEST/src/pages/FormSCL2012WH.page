        <style media="print">
            body { background-color:#fff; }
            #container .header { padding:0px; height:20px; border-top:70px #8dc63f solid; position:relative; }
            #container .header .logo { position:absolute; top:-65px; }
            #container .header .logo.efi { right:20px; }
            .formtable th { border-top:16px #F58025 solid; }
            .formtable th img { display:block; margin-top:-17px; max-width:480pt; }
            .formtable th span, h1 span { display:none; }
            .formtable td { padding:1px 5px; }
            .printonly { display:block !important; }
            .pagebreak-after { page-break-after:always; }
            .pagebreak-before { page-break-before:always; }
            #checklist { width:600px; margin:2em auto; }
            #checklist .text { margin:20px; }
            #checklist p { margin-bottom: 10px; }
            #checklist li { list-style:none; margin-bottom:4px; }
            #checklist .scl-header { width: 400px; }
            .error-icon, .question-icon, .button, #content-main .submitbutton { display:none !important; }
            #need-help { display:none; }
            input[type="text"] { background-color:#fff; }
            .screenonly { display:none; }
            h1, h2, #content-main, #content-top .text, .scl-header { margin: 0px; padding:0px; }
            h1 img { display:block; margin-top: -20px; }
            h1 { margin-bottom:5px; border-top:25px #F58025 solid }
