        <script>
            $(function() {
                $( ".datefield" ).datepicker();
            });
            $(document).ready(function(){ 
                $('.error-icon').tooltip({
                    track: true,
                    delay: 0,
                    showURL: false,
                    showBody: " - ",
                    fade: 250
                });
            });
        </script>
        <script>
            function checkMe() {
                // be careful in defining local tests - FDC runs rampant on form as written
                // so DOM ids and structure may be different from expected
                // transfer purchase date to FDC field
                if (document.getElementById("SCL2012WH:rebateForm:frmReadOnly").value == 'yes') return false;
                document.getElementById("SCL2012WH:rebateForm:frmPurchDate").value = document.getElementById("purchase_date").value
                document.getElementById("SCL2012WH:rebateForm:frmInstallDate").value = document.getElementById("install_date").value
                submitButton();
                return true;
            }
            function runOnLoad() {
                // if 'Other' status, redirect
                var sobj = document.getElementById("SCL2012WH:rebateForm:frmStatus");
                if (sobj.value == 'Other') {
                    var nurl = window.location.href;
                    window.location.href = nurl.replace("?Rebate","_Status?Rebate")+'&FormCode=SCLWH2012';
                    return;
                }
                // determine if there is any message to display (e.g. AJAX result)
                var obj = document.getElementById("SCL2012WH:rebateForm:frmLoadMessage");
                if (obj.value.length > 0) alert(obj.value);
				// determine if there is an account number error/warning, show dialog
				var eobj = document.getElementById("SCL2012WH:rebateForm:frmShowInvoice");
				if (eobj.value == 'Yes') {
					$('#frmAcctNumExampleDialog').dialog( { modal:true, width:750, });
				}
                // replace visible date from value exchanged w/server
                document.getElementById("purchase_date").value = document.getElementById("SCL2012WH:rebateForm:frmPurchDate").value;
                document.getElementById("install_date").value = document.getElementById("SCL2012WH:rebateForm:frmInstallDate").value;

//              if (document.getElementById("SCL2012WH:rebateForm:manualStore").value == "Y") {
//                  storeCheck(true);
//                  document.getElementById("manualstorecb").checked = true;
//              } else {
//                  storeCheck(false);
//                  document.getElementById("manualstorecb").checked = false;
//              }
                
                if (document.getElementById("SCL2012WH:rebateForm:manualModel").value == "Y") {
                    modelCheck(true);
                    document.getElementById("manualmodelcb").checked = true;
                } else {
                    modelCheck(false);
                    document.getElementById("manualmodelcb").checked = false;
                }
                
                var rn = document.getElementById("SCL2012WH:rebateForm:frmRebateNum").innerHTML;
                if (rn.length < 2) {
                    document.getElementById("refnumdiv").style.display = "none";
                } else {
                    document.getElementById("refnumdiv").style.display = "block";
                }
                if (document.getElementById("SCL2012WH:rebateForm:frmReadOnly").value == "yes") {
                    document.getElementById("purchase_date").disabled = true;
                    document.getElementById("install_date").disabled = true;
                    // document.getElementById("haulaway_date").disabled = true;
                    // document.getElementById("manualstorecb").disabled = true;
                    document.getElementById("manualmodelcb").disabled = true;
                    // document.getElementById("mailsamecb").disabled = true;   
                   jQuery('.submitbutton').addClass('disabled');
                }
                if (document.getElementById('SCL2012WH:rebateForm:frmAcctAddress1').value == '') { document.getElementById('SCL2012WH:rebateForm:frmAcctAddress1').value = 'Line 1'; } else if (document.getElementById('SCL2012WH:rebateForm:frmAcctAddress1').value != 'Line 1') { $('.address-field.frmAcctAddress1').addClass('entered'); }
                if (document.getElementById('SCL2012WH:rebateForm:frmAcctAddress2').value == '') { document.getElementById('SCL2012WH:rebateForm:frmAcctAddress2').value = 'Line 2'; } else if (document.getElementById('SCL2012WH:rebateForm:frmAcctAddress2').value != 'Line 2') { $('.address-field.frmAcctAddress2').addClass('entered'); }
                if (document.getElementById('SCL2012WH:rebateForm:frmMailAddress1').value == '') { document.getElementById('SCL2012WH:rebateForm:frmMailAddress1').value = 'Line 1'; } else if (document.getElementById('SCL2012WH:rebateForm:frmMailAddress1').value != 'Line 1') { $('.address-field.frmMailAddress1').addClass('entered'); }
                if (document.getElementById('SCL2012WH:rebateForm:frmMailAddress2').value == '') { document.getElementById('SCL2012WH:rebateForm:frmMailAddress2').value = 'Line 2'; } else if (document.getElementById('SCL2012WH:rebateForm:frmMailAddress2').value != 'Line 2') { $('.address-field.frmMailAddress2').addClass('entered'); }
                if (document.getElementById('SCL2012WH:rebateForm:frmStoreAddress1').value == '') { document.getElementById('SCL2012WH:rebateForm:frmStoreAddress1').value = 'Line 1'; } else if (document.getElementById('SCL2012WH:rebateForm:frmStoreAddress1').value != 'Line 1') { $('.address-field.frmStoreAddress1').addClass('entered'); }
                if (document.getElementById('SCL2012WH:rebateForm:frmStoreAddress2').value == '') { document.getElementById('SCL2012WH:rebateForm:frmStoreAddress2').value = 'Line 2'; } else if (document.getElementById('SCL2012WH:rebateForm:frmStoreAddress2').value != 'Line 2') { $('.address-field.frmStoreAddress2').addClass('entered'); }
                
                if (document.getElementById('SCL2012WH:rebateForm:frmUnHeatedSpace').value == 'Other') {
                    document.getElementById('manuhsdiv').style.display = "inline";
                } else {
                    document.getElementById('manuhsdiv').style.display = "none";
                }
            }
            function modelCheck(ischecked) {
                // alert(ischecked);
                var obj = document.getElementById("SCL2012WH:rebateForm:manualModel");
                if (ischecked) {
                    document.getElementById("selmandiv").style.display = "none";
                    document.getElementById("selmoddiv").style.display = "none";
                    document.getElementById("manmandiv").style.display = "block";
                    document.getElementById("manmoddiv").style.display = "block";
                    obj.value = "Y";
                } else {
                    document.getElementById("selmandiv").style.display = "block";
                    document.getElementById("selmoddiv").style.display = "block";
                    document.getElementById("manmandiv").style.display = "none";
                    document.getElementById("manmoddiv").style.display = "none";
                    obj.value = "N";
                }
            }
            // storeCheck not currently used.
            function storeCheck(ischecked) {
                // alert(ischecked);
                var obj = document.getElementById("SCL2012WH:rebateForm:manualStore");
                if (ischecked) {
                    document.getElementById("selstorediv").style.display = "none";
                    document.getElementById("manstorediv").style.display = "block";
                    obj.value = "Y";
                } else {
                    document.getElementById("selstorediv").style.display = "block";
                    document.getElementById("manstorediv").style.display = "none";
                    obj.value = "N";
                }
            }
            $(document).ready(function () {
                $('#seluhsdiv select').change(function() {
                    if ($(this).val() == 'Other') {
                        $('#manuhsdiv').css('display','inline-block');                      
                    }
                    else {
                        $('#manuhsdiv').css('display','none');  
                    }
                });
            });
            
            function hcCheck(ischecked) {
                var obj = document.getElementById("SCL2012WH:rebateForm:manualHC");
                if (ischecked) {
                    document.getElementById("selhcdiv").style.display = "none";
                    document.getElementById("manhcdiv").style.display = "block";
                    obj.value = "Y";
                }
                else {
                    document.getElementById("selhcdiv").style.display = "block";
                    document.getElementById("manhcdiv").style.display = "none";
                    obj.value = "N";
                }
            }
                        
            function focusValue(id,val) {
                if (document.getElementById("SCL2012WH:rebateForm:"+id).value == val) { 
                    document.getElementById("SCL2012WH:rebateForm:"+id).value = ''; 
                    $('.'+id).addClass('entered'); 
                }
                
            }
            
            function blurValue(id,val) {
                if (document.getElementById("SCL2012WH:rebateForm:"+id).value == '') { 
                    document.getElementById("SCL2012WH:rebateForm:"+id).value = val; 
                    $('.'+id).removeClass('entered'); 
                }
                
            }
            
            function fieldEntered(id,val) {
                if (document.getElementById("SCL2012WH:rebateForm:"+id).value != '' && document.getElementById("SCL2012WH:rebateForm:"+id).value != val) {
                    $('.'+id).addClass('entered');
                }
                else { 
                    $('.'+id).removeClass('entered');
                }
            }
            
            jQuery(document).ready(function($) {
                jQuery('#need-help').click(function() {
                    $('#help-dialog').dialog( { modal:true, width:500, });
                });
                jQuery('#seldp').append( $('#date-purchased') );
                jQuery('.question-icon').click(function() {
					$('#frmAcctNumExampleDialog').dialog( { modal:true, width:750, });
				});
            });
            
        </script>
    </head>
    <body onload="runOnLoad();">
        <div class="printonly pagebreak-after" id="checklist">
              <div class="header">
                <div class="scl-header">
                    <div class="logo"><img src="{!URLFOR($Resource.SCL2012, 'images/scl-logo.png')}" alt="Seattle City Light" style="float:left;" /></div>
                </div>
                <div class="efi-logo"><img src="{!URLFOR($Resource.SCL2012, 'images/efi-logo.gif')}" alt="EFI"  /></div>
            </div>
            <div class="text">
                <p>Thank you for submitting your Seattle City Light water heater rebate online.</p>
                <p>In order to finalize your rebate application, please do the following:</p>
                <ul>
                    <li><input type="checkbox" readonly="readonly" /> Print and sign the application form.</li>
                    <li><input type="checkbox" readonly="readonly" /> Attach a copy of your itemized receipt showing the rebated item. </li>
                    <li><input type="checkbox" readonly="readonly" /> Submit your application no later than 90 days after the purchase of your appliance.</li>
                </ul>
                <table>
                	<tr>
	                	<td style="padding-right:40px;">
			                <p>Mail to:</p>
			                <p>EFI &mdash; Seattle City Light WH Rebate<br />
			                40 Washington St., Suite 2000<br />
			                Westborough, MA 01581</p>
	                	</td>
	                	<td style="vertical-align:top;">
	                		<p>Questions?</p>
	                		<p>Contact us at 1-888-562-0120</p>
	                	</td>
                	</tr>
                </table>
            </div>
        </div>
        <div id="container">
            <div class="header">
                    <div class="logo"><img src="{!URLFOR($Resource.SCL2012, 'images/scl_wh_header.png')}" alt="Water Heater Rebate" style="float:left;" /></div>
                    <div class="logo efi"><img src="{!URLFOR($Resource.SCL2012, 'images/scl_logo.png')}" alt="Seattle City Light" style="float:right;" /></div>
            </div>
            <div id="main">
                <div id="content-top">
                    <div id="need-help" class="screenonly">Need help?</div>
                    <h1>
                        <span>Get a $250 rebate on qualified models.</span>
                        <img src="{!URLFOR($Resource.SCL2012, 'images/scl_header.png')}" alt="Get a $50 rebate on a qualified model when you recycle your old unit." />
                    </h1>
                    <div class="text">
                        <h2 class="orange">To apply, follow these steps:</h2>
                        <ol class="orange">
                            <li><span class="normal"><strong>Your new heat pump water heater must be purchased between January 1, 2012 and December 31, 2012.</strong></span></li>
                            <li><span class="normal">Read the Terms and Conditions on the below to ensure you qualify.</span></li>
                            <li><span class="normal">Complete the rebate form and attach a <strong>copy</strong> of your itemized receipt. Receipt must include retailer/installer name,
                            address, phone number, purchase date, price, product manufacturer and model number.</span></li>
                            <li><span class="normal">Sign and date the rebate form and send the completed form with itemized receipt postmarked <strong>within 90 days of purchase date</strong> to:
                            </span></li>
                            <div class="normal">
                                <div class="mailto left">
                                    <p><span class="orange">Mail</span><br />
                                    EFI &mdash; Seattle City Light WH Rebate<br />
                                    40 Washington St., Suite 2000<br />
                                    Westborough, MA 01581</p>
                                </div>
                                <div class="mailto right">
                                    <p><span class="orange">For more information:</span><br />
                                    Email: SeattleAppliances@efi.org<br />
                                    Website: seattle.gov/appliances<br />
                                    Questions? Call 1-888-562-0120</p>
                                </div>
                            </div>
                        </ol>
                    </div>
                    <div id="help-dialog" class="scl-dialog" title="Need help?">For additional information or assistance in completing this online application, please contact our customer care center at 1-888-562-0120. We are open from 5 AM to 5 PM, Monday through Friday, and 5 AM to 2 PM on Saturdays.</div>
                </div>
                <div id="content-main">
                    <h2>All information below is REQUIRED for processing.</h2>
                    <apex:form id="rebateForm">
                        <apex:actionFunction name="submitButton" action="{!actionCheckMe}" />
                        <apex:inputHidden id="frmLoadMessage" value="{!frmLoadMessage}" />
		                <apex:inputHidden id="frmReadOnly" value="{!frmReadOnly}" />
          				<apex:inputHidden id="frmShowInvoice" value="{!frmShowInvoice}" />
        		        <apex:inputHidden id="frmStatus" value="{!frmStatus}" />
                        <div id="refnumdiv" style="font-weight:bold;">
                            <label>Reference #: </label>
                            <span class="mediumshorttext noborder">
                                <apex:outputText id="frmRebateNum" value="{!frmRebateNum}" styleClass="noborder"/>
                            </span>
                        </div>
                        <table class="formtable"  cellpadding="0" cellspacing="0" border="0">
                        <tbody>
                            <tr><th colspan="3"><span class="text">Customer Information</span><img src="{!URLFOR($Resource.SCL2012, 'images/cust_info.png')}" alt="customer information" /></th></tr>
                            <tr>
                                <td colspan="3" class="nopadding">
                                    <table cellpadding="0" cellspacing="0" border="0" width="100%">
                                        <tr>
                                        <td width="70%" style="border-right:1px #f58025 solid;">
                                            <label>First</label>
                                            <span style="margin-right:20px;"><apex:inputText id="frmFirstName" value="{!frmFirstName}" disabled="{!disableDE}"  maxlength="25" size="25" tabindex="1" /></span>
                                            <apex:image value="{!URLFOR($Resource.SCL2012, 'images/warning-icon-yellow.png')}" alt="warning" title="{!frmFirstNameMsg}" styleClass="error-icon" id="frmFirstNameErr" style="{!frmFirstNameMsgStyle}" />
                                            <label>Last</label>
                                            <span><apex:inputText id="frmLastName" value="{!frmLastName}" disabled="{!disableDE}"  maxlength="25" size="25" tabindex="2" /></span>
                                            <apex:image value="{!URLFOR($Resource.SCL2012, 'images/warning-icon-red.png')}" alt="error" title="{!frmLastNameMsg}" styleClass="error-icon" id="frmLastNameErr" style="{!frmLastNameMsgStyle}" />
                                        </td>
