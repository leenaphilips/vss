                } else {
                    document.getElementById("selstorediv").style.display = "block";
                    document.getElementById("manstorediv").style.display = "none";
                    // obj.value = "N";
                }
            }
            
            function doSubmitAs(opt) {
                // alert(opt);
                if (opt == 'Reservation') {
                    document.getElementById("as_app").checked = false;
                    document.getElementById("as_res").checked = true;
                    document.getElementById("subviarow").style.display = "none";
                    document.getElementById("emailreqrow").style.display = "none";
                    document.getElementById("suptdochrow").style.display = "none";
                    document.getElementById("suptdocrow").style.display = "none";
                    $('.blankstophard').removeClass('hardstop'); 
                    $('.blankstopsoft').removeClass('softstop'); 
                } else {
                    document.getElementById("as_app").checked = true;
                    document.getElementById("as_res").checked = false;
                    document.getElementById("subviarow").style.display = "table-row";
                    doSubmitVia(document.getElementById("SGE2012HE:rebateForm:submitVia").value);
                    $('.blankstophard').addClass('hardstop'); 
                    $('.blankstopsoft').addClass('softstop'); 
                }
                document.getElementById("SGE2012HE:rebateForm:submitAs").value = opt;
                updateMessage();
            }

            function doSubmitVia(opt) {
                // alert(opt);
                if (opt == 'Electronic') {
                    document.getElementById("via_mail").checked = false;
                    document.getElementById("via_elect").checked = true;
                    document.getElementById("SGE2012HE:rebateForm:emailRequest").checked = false; // initialize
                    if (documentCount == 0) {
                        document.getElementById("emailreqrow").style.display = "table-row";
                    }
                    document.getElementById("suptdochrow").style.display = "table-row";
                    document.getElementById("suptdocrow").style.display = "table-row";
                } else {
                    document.getElementById("via_mail").checked = true;
                    document.getElementById("via_elect").checked = false;
                    document.getElementById("SGE2012HE:rebateForm:emailRequest").checked = false; // clear it
                    document.getElementById("emailreqrow").style.display = "none";
                    document.getElementById("suptdochrow").style.display = "none";
                    document.getElementById("suptdocrow").style.display = "none";
                }
                document.getElementById("SGE2012HE:rebateForm:submitVia").value = opt;
                updateMessage();
            }
            
            function useOptions() {
                // alert("use");
                document.getElementById("opttable").style.display = "none";
                document.getElementById("seltable").style.display = "table";
                document.getElementById("formtable").style.display = "table";
                document.getElementById("tctable").style.display = "table";
                document.getElementById("submitbutton").style.display = "block";
                document.getElementById("termsconditions").style.display = "table";
            }
            
            function changeOptions() {
                // alert("change");
                document.getElementById("opttable").style.display = "table";
                document.getElementById("seltable").style.display = "none";
                document.getElementById("formtable").style.display = "none";
                document.getElementById("tctable").style.display = "none";
                document.getElementById("submitbutton").style.display = "none";
                document.getElementById("termsconditions").style.display = "none";
            }
            
            function updateMessage() {
                var msg;
                msg = "You are submitting a <span class='msgcontent tcolorbk'>"+document.getElementById("SGE2012HE:rebateForm:submitAs").value+"</span>";
                if (document.getElementById("SGE2012HE:rebateForm:submitAs").value == 'Rebate') {
                    msg = msg +"; and submitting via <span class='msgcontent tcolorbk'>"+
                    document.getElementById("SGE2012HE:rebateForm:submitVia").value+"</span>";
                }
                // alert(msg);
                document.getElementById("messagediv").innerHTML = msg;
            }
            
            function updateDocumentCount(n) {
                // the upload area is visible if this was called
                documentCount = n; // if needed later
                if (n > 0) {
                    document.getElementById("emailreqrow").style.display = "none";
                    document.getElementById("SGE2012HE:rebateForm:emailRequest").checked = false; // just in case
                } else {
                    document.getElementById("emailreqrow").style.display = "table-row";
                }
            }
            
            function requestEmailCheck(ischecked) {
                if (ischecked) {
                    document.getElementById("suptdochrow").style.display = "none";
                    document.getElementById("suptdocrow").style.display = "none";
                } else {
                    document.getElementById("suptdochrow").style.display = "table-row";
                    document.getElementById("suptdocrow").style.display = "table-row";
                }
            }
            
                        
            function focusValue(id,val) {
                if (document.getElementById("SGE2012HE:rebateForm:"+id).value == val) { 
                    document.getElementById("SGE2012HE:rebateForm:"+id).value = ''; 
                    $('.'+id).addClass('entered'); 
                }
                
            }
            
            function blurValue(id,val) {
                if (document.getElementById("SGE2012HE:rebateForm:"+id).value == '') { 
                    document.getElementById("SGE2012HE:rebateForm:"+id).value = val; 
                    $('.'+id).removeClass('entered'); 
                }
                
            }
            
            function fieldEntered(id,val) {
                if (document.getElementById("SGE2012HE:rebateForm:"+id).value != '' && document.getElementById("SGE2012HE:rebateForm:"+id).value != val) {
                    $('.'+id).addClass('entered');
                }
                else { 
                    $('.'+id).removeClass('entered');
                }
            }
            
