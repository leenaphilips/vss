            var frmColor2Light = changeColor(frmColor2,'light',.9);
            var colorStyling = ' .pcolor { color:'+frmColor1+'; } .scolor { color:'+frmColor2+'; } ' +
                ' .pcolorbk { background-color:'+frmColor1+'; } .scolorbk { background-color:'+frmColor2+'; }   ' +
                ' .pcolorbr { border-color:'+frmColor1+'; } .scolorbr { border-color:'+frmColor2+'; } ' +
                ' .tcolor { background-color:'+frmColor3+'; } .tcolorbk { background-color:'+frmColor3+'; } .tcolorbr { border-color:'+frmColor3+'; } ' +
                ' .formtable { border-color:'+frmColor1+'; } ' +
                ' .formtable th { background-color:'+frmColor1+'; border-color:'+frmColor1+'; } .formtable td { border-bottom-color:'+frmColor1+'; border-right-color:'+frmColor1+'; } ' +
                ' .shaded td {  } ' +
                ' #content-main .roundedbutton { filter: progid:DXImageTransform.Microsoft.gradient(startColorstr="'+frmColor2+'", endColorstr="'+frmColor2Dark+'"); ' +
                    ' background: -webkit-gradient(linear, left top, left bottom, from('+frmColor2+'), to('+frmColor2Dark+')); ' +
                    ' background: -moz-linear-gradient(top,  '+frmColor2+',  '+frmColor2Dark+'); ' +
                    ' border-color:'+frmColor2Dark+'; } ' +
                ' #content-main .roundedbutton::after {  } ' +
                ' #content-main .roundedbutton:hover { background:'+frmColor2Dark+'; } ';
            document.write ('<style>' + colorStyling + '</style>');
            
            // functions for lightening and darkening primary and secondary colors for gradients, shading, etc.
            function lightenRgb(r,g,b,n) {
                var n2 = 1 + n;
                var rgb = Array();
                rgb[0] = r*n2 < 255 ? Math.round(r*n2) : 255;
                rgb[1] = g*n2 < 255 ? Math.round(g*n2) : 255;
                rgb[2] = b*n2 < 255 ? Math.round(b*n2) : 255;
                return rgb;
            }
            function darkenRgb(r,g,b,n) {
                var rgb = Array();
                rgb[0] = Math.round(r * n);
                rgb[1] = Math.round(g * n);
                rgb[2] = Math.round(b * n);
                return rgb;
            }
            function hexToRgb(hex) {
                var rgb = Array();
                var htr_result = /^#?([a-f\d]{2})([a-f\d]{2})([a-f\d]{2})$/i.exec(hex);
                rgb[0] = parseInt(htr_result[1], 16);
                rgb[1] = parseInt(htr_result[2], 16);
                rgb[2] = parseInt(htr_result[3], 16);
                return rgb;
            }
            function componentToHex(c) {
                var hex = c.toString(16);
                return hex.length == 1 ? "0" + hex : hex;
            }
            function rgbToHex(r, g, b) {
                return "#" + componentToHex(r) + componentToHex(g) + componentToHex(b);
            }
            function changeColor(hex,dir,pct) {
                var rgb = hexToRgb(hex);
                var rgb2 = dir == 'dark' ? darkenRgb(rgb[0],rgb[1],rgb[2],pct) : lightenRgb(rgb[0],rgb[1],rgb[2],pct);
                var hex2 = rgbToHex(rgb2[0],rgb2[1],rgb2[2]);
                return hex2;
            }
             
