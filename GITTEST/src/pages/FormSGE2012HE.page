            function modelCheck(prod, ischecked) {
                if (ischecked) {
                    document.getElementById("selmandiv"+prod).style.display = "none";
                    document.getElementById("selmoddiv"+prod).style.display = "none";
                    document.getElementById("manmandiv"+prod).style.display = "block";
                    document.getElementById("manmoddiv"+prod).style.display = "block";
                } else {
                    document.getElementById("selmandiv"+prod).style.display = "block";
                    document.getElementById("selmoddiv"+prod).style.display = "block";
                    document.getElementById("manmandiv"+prod).style.display = "none";
                    document.getElementById("manmoddiv"+prod).style.display = "none";
                }
