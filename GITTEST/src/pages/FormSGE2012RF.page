        <script>
            function checkMe() {
                // be careful in defining local tests - FDC runs rampant on form as written
                // so DOM ids and structure may be different from expected
                // transfer purchase date to FDC field
                if (document.getElementById("SGE2012RF:rebateForm:frmReadOnly").value == 'yes') return false;
                document.getElementById("SGE2012RF:rebateForm:frmPurchDate").value = document.getElementById("purchase_date").value
                submitButton();
                return true;
            }
            function runOnLoad() {
                // if 'Other' status, redirect
                var sobj = document.getElementById("SGE2012RF:rebateForm:frmStatus");
                if (sobj.value == 'Other') {
                    var nurl = window.location.href;
                    window.location.href = nurl.replace("?Rebate","_Status?Rebate")+'&FormCode=SGERF2012';
                }
                // determine if there is any message to display (e.g. AJAX result)
                var obj = document.getElementById("SGE2012RF:rebateForm:frmLoadMessage");
                if (obj.value.length > 0) alert(obj.value);
                // replace visible date from value exchanged w/server
                document.getElementById("purchase_date").value = document.getElementById("SGE2012RF:rebateForm:frmPurchDate").value;

                if (document.getElementById("SGE2012RF:rebateForm:manualStore").value == "Y") {
                    storeCheck(true);
                    document.getElementById("manualstorecb").checked = true;
                } else {
                    storeCheck(false);
                    document.getElementById("manualstorecb").checked = false;
                }
                
                if (document.getElementById("SGE2012RF:rebateForm:manualModel").value == "Y") {
                    modelCheck(true);
                    document.getElementById("manualmodelcb").checked = true;
                } else {
                    modelCheck(false);
                    document.getElementById("manualmodelcb").checked = false;
                }
                    
                var rn = document.getElementById("SGE2012RF:rebateForm:frmRebateNum").innerHTML;
                if (rn.length < 2) {
                    document.getElementById("refnumdiv").style.display = "none";
                } else {
                    document.getElementById("refnumdiv").style.display = "block";
                }
                if (document.getElementById("SGE2012RF:rebateForm:frmReadOnly").value == "yes") {
                    document.getElementById("purchase_date").disabled = true;
                    document.getElementById("manualstorecb").disabled = true;
                    document.getElementById("manualmodelcb").disabled = true;
                    jQuery('.submitbutton').addClass('disabled');
                    // document.getElementById("mailsamecb").disabled = true;   
                }
                
               
                if (document.getElementById('SGE2012RF:rebateForm:frmAcctAddress1').value == '') { document.getElementById('SGE2012RF:rebateForm:frmAcctAddress1').value = 'Line 1'; } else if (document.getElementById('SGE2012RF:rebateForm:frmAcctAddress1').value != 'Line 1') { $('.address-field.frmAcctAddress1').addClass('entered'); }
                if (document.getElementById('SGE2012RF:rebateForm:frmAcctAddress2').value == '') { document.getElementById('SGE2012RF:rebateForm:frmAcctAddress2').value = 'Line 2'; } else if (document.getElementById('SGE2012RF:rebateForm:frmAcctAddress2').value != 'Line 2') { $('.address-field.frmAcctAddress2').addClass('entered'); }
                if (document.getElementById('SGE2012RF:rebateForm:frmMailAddress1').value == '') { document.getElementById('SGE2012RF:rebateForm:frmMailAddress1').value = 'Line 1'; } else if (document.getElementById('SGE2012RF:rebateForm:frmMailAddress1').value != 'Line 1') { $('.address-field.frmMailAddress1').addClass('entered'); }
                if (document.getElementById('SGE2012RF:rebateForm:frmMailAddress2').value == '') { document.getElementById('SGE2012RF:rebateForm:frmMailAddress2').value = 'Line 2'; } else if (document.getElementById('SGE2012RF:rebateForm:frmMailAddress2').value != 'Line 2') { $('.address-field.frmMailAddress2').addClass('entered'); }
                if (document.getElementById('SGE2012RF:rebateForm:frmStoreAddress1').value == '') { document.getElementById('SGE2012RF:rebateForm:frmStoreAddress1').value = 'Line 1'; } else if (document.getElementById('SGE2012RF:rebateForm:frmStoreAddress1').value != 'Line 1') { $('.address-field.frmStoreAddress1').addClass('entered'); }
                if (document.getElementById('SGE2012RF:rebateForm:frmStoreAddress2').value == '') { document.getElementById('SGE2012RF:rebateForm:frmStoreAddress2').value = 'Line 2'; } else if (document.getElementById('SGE2012RF:rebateForm:frmStoreAddress2').value != 'Line 2') { $('.address-field.frmStoreAddress2').addClass('entered'); }
               
            }
            function modelCheck(ischecked) {
                // alert(ischecked);
                var obj = document.getElementById("SGE2012RF:rebateForm:manualModel");
                if (ischecked) {
                    document.getElementById("selmandiv").style.display = "none";
                    document.getElementById("selmoddiv").style.display = "none";
                    document.getElementById("manmandiv").style.display = "block";
                    document.getElementById("manmoddiv").style.display = "block";
                    obj.value = "Y";
                } else {
                    document.getElementById("selmandiv").style.display = "block";
                    document.getElementById("selmoddiv").style.display = "block";
                    document.getElementById("manmandiv").style.display = "none";
                    document.getElementById("manmoddiv").style.display = "none";
                    obj.value = "N";
                }
            }
            function storeCheck(ischecked) {
                // alert(ischecked);
                var obj = document.getElementById("SGE2012RF:rebateForm:manualStore");
                if (ischecked) {
                    jQuery('#mandp').append( $('#date-purchased') );
                    document.getElementById("selstorediv").style.display = "none";
                    document.getElementById("manstorediv").style.display = "block";
                    obj.value = "Y";
                } else {
                    jQuery('#seldp').append( $('#date-purchased') );
                    document.getElementById("selstorediv").style.display = "block";
                    document.getElementById("manstorediv").style.display = "none";
                    obj.value = "N";
                }
            }
            function hcCheck(ischecked) {
                var obj = document.getElementById("SGE2012RF:rebateForm:manualHC");
                // alert(ischecked);
                if (ischecked) {
                    document.getElementById("selhcdiv").style.display = "none";
                    document.getElementById("manhcdiv").style.display = "block";
                    obj.value = "Y";
                }
                else {
                    document.getElementById("selhcdiv").style.display = "block";
                    document.getElementById("manhcdiv").style.display = "none";
                    obj.value = "N";
                }
            }
                        
            function focusValue(id,val) {
                if (document.getElementById("SGE2012RF:rebateForm:"+id).value == val) { 
                    document.getElementById("SGE2012RF:rebateForm:"+id).value = ''; 
                    $('.'+id).addClass('entered'); 
                }
                
            }
            
            function blurValue(id,val) {
                if (document.getElementById("SGE2012RF:rebateForm:"+id).value == '') { 
                    document.getElementById("SGE2012RF:rebateForm:"+id).value = val; 
                    $('.'+id).removeClass('entered'); 
                }
                
            }
            
            function fieldEntered(id,val) {
                if (document.getElementById("SGE2012RF:rebateForm:"+id).value != '' && document.getElementById("SGE2012RF:rebateForm:"+id).value != val) {
                    $('.'+id).addClass('entered');
                }
                else { 
                    $('.'+id).removeClass('entered');
                }
            }
            
            jQuery(document).ready(function($) {
                jQuery('#need-help').click(function() {
                    $('#help-dialog').dialog( { modal:true, width:500, });
                });
                jQuery('#seldp').append( $('#date-purchased') );
                jQuery('.question-icon').click(function() {
                    $('#frmAcctNumExampleDialog').dialog( { modal:true, width:750, });
                });
            });
            

            
        </script>
    </head>
